import * as assert from 'assert';

console.log("Example:");
console.log(countWords("How aresjfhdskfhskd you?", ["how", "are", "you", "hello"]))

assert.equal(countWords("How aresjfhdskfhskd you?", ["how", "are", "you", "hello"]), 3);
assert.equal(countWords("Bananas, give me bananas!!!", ["banana", "bananas"]), 2);
assert.equal(countWords("Lorem ipsum dolor sit amet, consectetuer adipiscing elit.", ["sum", "hamlet", "infinity", "anything"]), 1);
